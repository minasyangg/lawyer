    "@upstash/redis": "^1.35.4",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "next": "15.5.0",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "prisma": "^6.14.0",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
